{
  "$schema": "./node_modules/@angular/cli/lib/config/schema",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "RisingEnterprise": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/RisingEnterprise",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "lib/jquery.Thailand.js/dist/jquery.Thailand.min.css",
              "node_modules/font-awesome-scss/scss/font-awesome.scss",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.scss",
              "src/scss/style.scss",
              "src/scss/widget.scss",
              "src/scss/pages.scss",
              "src/assets/icon/feather/css/feather.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "lib/jquery.Thailand.js/dependencies/JQL.min.js",
              "lib/jquery.Thailand.js/dependencies/typeahead.bundle.js",
              "lib/jquery.Thailand.js/src/jquery.Thailand.js",
              "node_modules/xlsx/dist/xlsx.full.min.js",
              "node_modules/clipboard/dist/clipboard.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "RisingEnterprise:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "RisingEnterprise:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RisingEnterprise:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "RisingEnterprise-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "RisingEnterprise:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "RisingEnterprise:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "RisingEnterprise"
}